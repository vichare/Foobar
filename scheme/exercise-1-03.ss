(include "assert.ss")
(display "Return the summation of 2 maximum numbers among 3.\n")
(define (summax2 a b c)
  (if (> a b) 
    (if (> c b)
      (+ a c)
      (+ a b)
    )
    (if (> c a)
      (+ b c)
      (+ a b)
    )
  )
)
(unittest (summax2 2 3 4) 7 1)
(unittest (summax2 3 2 4) 7 2)
(unittest (summax2 2 4 3) 7 3)
(unittest (summax2 4 2 3) 7 4)
(unittest (summax2 3 4 2) 7 5)
(unittest (summax2 4 3 2) 7 6)
(unittest (summax2 1 1 1) 2 7)
(unittest (summax2 2 1 2) 4 8)
(display "\n")


